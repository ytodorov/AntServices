@{
    ViewBag.Title = "Free distributed network tools and services.";
    ViewBag.MetaDescription = "We offer ping, traceroute, " +
        "port scan, barcode generation, IP location, word processing and excel processing services.";   
}
<div class="normalheader transition animated fadeIn small-header">
    <div class="hpanel">
        <div class="panel-body">
            <div id="hbreadcrumb" class="pull-right">
                <ol class="hbreadcrumb breadcrumb">
                    <li>
                        <a href="/">Home</a>
                    </li>
                </ol>
            </div>
            <h2 class="font-light m-b-xs">
                Home
            </h2>
            <small>Distributed network services</small>
        </div>
    </div>
</div>
<section id="features2" class="landing-page icon-big">
    <div class="container">
        <div class="row text-center">
            <a href="~/ping">
                <div class="col-md-4" title="Ping">
                    <h3 class="m-t-lg">
                        <i class="pe-7s-sun text-success icon-big"></i>
                    </h3>
                    <strong>Ping</strong>
                    <p>Free distributed online ping service. Ping simultaneously from 10 locations to check if a host is online and measure round-trip times.</p>
                </div>
            </a>
            <a href="~/traceroute" >
                <div class="col-md-4" title="Traceroute">
                    <h3 class="m-t-lg">
                        <i class="pe-7s-science text-success icon-big"></i>
                    </h3>
                    <strong>Traceroute</strong>
                    <p>Free distributed online traceroute service. Display the route and measure transit delays of packets simultaneously from 10 locations.</p>
                </div>
            </a>
            <a href="~/portscan" >
                <div class="col-md-4" title="Port scan">
                    <h3 class="m-t-lg">
                        <i class="pe-7s-shuffle text-success icon-big"></i>
                    </h3>
                    <strong>Port scan</strong>
                    <p>Free online port scan service. Probe a server or host for open ports.</p>
                </div>
            </a>
        </div>

        <div class="row text-center">

            <a href="~/barcode">
                <div class="col-md-4" title="Barcode generation">
                    <h3 class="m-t-lg">
                        <i class="fa fa-barcode text-success icon-big"></i>
                    </h3>
                    <strong>Barcode generation</strong>
                    <p>Generate barcodes in 20 encodings simultaneously and export them in PDF, image or SVG.</p>
                </div>
            </a>

            <a href="~/word" >
                <div class="col-md-4" title="Words processing">
                    <h3 class="m-t-lg">
                        <i class="fa fa-file-word-o text-success icon-big"></i>
                    </h3>
                    <strong>Words processing</strong>
                    <p>Create, import and export DOCX, HTML, RTF and TXT documents, and export them to PDF.</p>
                </div>
            </a>
            <a href="~/spread" >
                <div class="col-md-4" title="Spread processing">
                    <h3 class="m-t-lg">
                        <i class="fa fa-file-excel-o text-success icon-big"></i>
                    </h3>
                    <strong>Spread processing</strong>
                    <p>Create, import and export XLSX, CSV and TXT spreadsheet documents, and export them to PDF.</p>
                </div>
            </a>

          

        </div>
        @*<div class="row text-center">
            <a href="~/css" >

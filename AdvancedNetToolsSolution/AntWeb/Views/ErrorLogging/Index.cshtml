@model ErrorLoggingViewModel

@{
    ViewBag.Title = "| Errors Log";
    ViewBag.MetaDescription = "Error Logging page of the Free distributed web-based ping service. Pings simultaneously from many locations to check if a host is online and measure round-trip times.";
}
<div class="row text-center">
    <div class="col-lg-12 text-primary">
        <h1><span>Error Log</span></h1>
    </div>
</div>

   <script>
        function getAllErrorLogs() {
            var data =
            {
                allErrorLogs: true
            };
            return data;
        }
</script>
@Html.Raw((Html.AntCreateStandardPanel("hviolet", "Error logs for the last 10 errors", "<p>" +
     Html.Kendo().Grid<ErrorLoggingViewModel>().CreateErrorLoggingGrid(Url, "getAllErrorLogs").ToHtmlString() + "</p>")))
<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="@ViewBag.MetaDescription">
    <title>Tools for Net @ViewBag.Title</title>

    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700' rel='stylesheet' type='text/css'>
    

    <link rel="icon" type="image/x-icon" href="~/favicon.ico">
    @*<link rel="shortcut icon" href="favicon.ico">*@
    <!-- Primary vendor style -->
    @Styles.Render(paths: "~/bundles/font-awesome/css")
    @Styles.Render(paths: "~/bundles/animate/css")
    @Styles.Render(paths: "~/bundles/bootstrap/css")
    @Styles.Render(paths: "~/bundles/peicon7stroke/css")
    @Styles.Render(paths: "~/bundles/ladda/css")
    @Styles.Render(paths: "~/bundles/toastr/css")

    <!-- Add local styles, mostly for plugins css file -->
    @if (IsSectionDefined(name: "Styles"))
    {@RenderSection(name: "Styles", required: false)}

    <!-- Primary HOMER style -->
    @Styles.Render(paths: "~/bundles/homer/css")

    <link rel="stylesheet" href="http://kendo.cdn.telerik.com/2016.1.226/styles/kendo.common.min.css">
    <link rel="stylesheet" href="http://kendo.cdn.telerik.com/2016.1.226/styles/kendo.metro.min.css">
</head>
<body class="@Html.PageClass() fixed-footer hide-sidebar">
  
    <!-- Simple splash screen-->
    <div class="splash">
        <div class="color-line"></div>
        <div class="splash-title text-success">
            <p><h3>Tools for Net</h3></p>
            <p><h4> Distributed network services</h4></p>
            <p><h5>Loading ... Please wait</h5></p>
            <div class="spinner">
                <div class="rect1"></div>
                <div class="rect2"></div>
                <div class="rect3"></div>
                <div class="rect4"></div>
                <div class="rect5"></div>
            </div>
        </div>
    </div>
    <!--[if lt IE 7]>
    <p class="alert alert-danger">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->
    <!-- Header -->
    <div id="header" class="light-version">
        @Html.Partial("_Header")
    </div>

    <!-- Navigation -->
    <aside id="menu">
        @Html.Partial("_Navigation")
    </aside>

    <!-- Main view  -->
    <div id="wrapper">
        <div id="divBody">
            @RenderBody()
        </div>
        <!-- Footer-->
        <div id="divFooter">
            @Html.Partial("_Footer")
        </div>
    </div>
    <script src="~/Scripts/googleAnalytics.js"></script>
    <!-- Section for main scripts render -->
    @*@Scripts.Render("~/bundles/jquery/js")*@
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDoER6oaWJAUFMNCCE8IkCGsIyHsbTE0Zw"></script>
    <script src="http://code.jquery.com/jquery-1.9.1.min.js"></script>
    <script src="~/Scripts/jquery.cookie.js"></script>
    @Scripts.Render("~/bundles/bootstrap/js")
    @Scripts.Render("~/bundles/homer/js")
    @Scripts.Render("~/bundles/ladda/js")
    @Scripts.Render("~/bundles/toastr/js")
    <script src="http://kendo.cdn.telerik.com/2016.1.226/js/jszip.min.js"></script>


    <script src="http://kendo.cdn.telerik.com/2016.1.226/js/kendo.all.min.js"></script>
    <script src="http://kendo.cdn.telerik.com/2016.1.226/js/kendo.aspnetmvc.min.js"></script>
    <script src="http://kendo.cdn.telerik.com/2016.1.226/js/pako_deflate.min.js"></script>



    <!-- Handler for local scripts -->
    @RenderSection("scripts", required: false)

    @Html.Kendo().DeferredScripts()
    <script src="~/Scripts/clipboard/clipboard.min.js"></script>
    <script src="~/Scripts/yk.js"></script>
    <script type="text/javascript">
    window.cookieconsent_options = {"message":"This website uses cookies to ensure you get the best experience on our website","dismiss":"Got it!","learnMore":"More info","link":null,"theme":"light-top"};
    </script>
    <script type="text/javascript" src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-571b2acff4b44bbf"></script>
</body>
</html>
